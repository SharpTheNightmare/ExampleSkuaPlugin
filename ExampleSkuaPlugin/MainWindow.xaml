<wpf:CustomWindow
        x:Name="Main" x:Class="ExamplePlugin.MainWindow"
        xmlns:wpf="clr-namespace:Skua.WPF;assembly=Skua.WPF" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamplePlugin"
        mc:Ignorable="d"
        Style="{StaticResource CustomWindow}"
        Title="Example Plugin" Height="357" Width="363"
        Closing="MainWindow_Closing">
    <Grid>
        <Button x:Name="NameChangeButton" Content="Button" HorizontalAlignment="Center" Margin="0,171,0,0" VerticalAlignment="Top" Height="50" Width="152"/>
        <TextBox x:Name="NameChangeTxt" HorizontalAlignment="Center" Margin="0,116,0,0" Text="Test" VerticalAlignment="Top" Width="153" Height="50" TextAlignment="Center" FontSize="16" TextWrapping="Wrap"/>

    </Grid>
</wpf:CustomWindow>
